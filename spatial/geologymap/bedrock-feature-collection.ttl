PREFIX dcat: <http://www.w3.org/ns/dcat#>
PREFIX dcterms: <http://purl.org/dc/terms/>
PREFIX geo: <http://www.opengis.net/ont/geosparql#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX schema: <https://schema.org/>
PREFIX xsd: <http://www.w3.org/2001/XMLSchema#>

<http://data.bgs.ac.uk/id/geology625k>
    rdfs:member <http://data.bgs.ac.uk/id/geology625k/bedrock> ;
.

<http://data.bgs.ac.uk/id/geology625k/bedrock>
    a geo:FeatureCollection ;
    dcterms:temporal [
            a dcterms:PeriodOfTime ;
            dcat:startDate "2011-11-11"^^xsd:date
        ] ;
    geo:hasBoundingBox [
            a geo:Geometry ;
            geo:asWKT "<http://www.opengis.net/def/crs/OGC/1.3/CRS84> POLYGON ((3 49.5, 3 61, -9 61, -9 49.5, 3 49.5))"^^geo:wktLiteral
        ] ;
    dcat:keyword
        "britain" ,
        "geology" ;
    schema:description "Bedrock features of the generalised digital geological map data based on BGS's published poster maps of the UK (North and South). Bedrock related themes created by generalisation of 1:50 000 data to make the 2007 fifth edition Bedrock Geology map." ;
    schema:name "BGS Geology 625k Bedrock" ;
.

