dcterms:conformsTo
    schema:description "An established standard to which the described resource conforms."@en ;
    schema:name "Conforms To"@en ;
.

dcterms:created
    schema:description "Date of creation of the resource."@en ;
    schema:name "Date Created"@en ;
.

dcterms:creator
    schema:description "An entity responsible for making the resource."@en ;
    schema:name "Creator"@en ;
.

dcterms:identifier
    schema:description "An unambiguous reference to the resource within a given context."@en ;
    schema:name "Identifier"@en ;
.

dcterms:modified
    schema:description "Date on which the resource was changed."@en ;
    schema:name "Date Modified"@en ;
.

dcterms:publisher
    schema:description "An entity responsible for making the resource available."@en ;
    schema:name "Publisher"@en ;
.

dcterms:source
    schema:description "A related resource from which the described resource is derived."@en ;
    schema:name "Source"@en ;
.

<http://purl.org/linked-data/registry#status>
    schema:description "The status of this register entry"@en ;
    schema:name "status"@en ;
.

skos:Concept
    schema:description
        "An idea or notion; a unit of thought" ,
        "An idea or notion; a unit of thought"@en ;
    schema:name
        "Concept" ,
        "Concept"@en ;
.

skos:ConceptScheme
    schema:description "A set of concepts, optionally including statements about semantic relationships between those concepts"@en ;
    schema:name "Concept Scheme"@en ;
.

skos:definition
    schema:description "A statement or formal explanation of the meaning of a concept"@en ;
    schema:name "definition"@en ;
.

skos:hasTopConcept
    schema:description "Relates, by convention, a concept scheme to a concept which is topmost in the broader/narrower concept hierarchies for that scheme, providing an entry point to these hierarchies"@en ;
    schema:name "has top concept"@en ;
.

skos:historyNote
    schema:description "A note about the past state/use/meaning of a concept"@en ;
    schema:name "history note"@en ;
.

skos:inScheme
    schema:description "Relates a resource (for example a concept) to a concept scheme in which it is included"@en ;
    schema:name "is in scheme"@en ;
.

skos:prefLabel
    schema:description "The preferred lexical label for a resource, in a given language"@en ;
    schema:name "preferred label"@en ;
.

prof:hasArtifact
    schema:description "The URL of a downloadable file with particulars such as its format and role indicated by the Resource Descriptor"@en ;
    schema:name "has artifact" ;
.

prof:hasResource
    schema:description "A resource which describes the nature of an artifact and the role it plays in relation to the Profile"@en ;
    schema:name "has resource"@en ;
.

prof:hasRole
    schema:description "The function of an artifact described by a Resource Descriptor, such as specification, guidance etc."@en ;
    schema:name "has role" ;
.

<https://kurrawong.ai>
    schema:description """KurrawongAI is a small, Australian-based company enabling organisations to take control of their data.

We use data modelling, data governance and data systems implementation expertise all based on Semantic Web and Knowledge Graph principles to ensure data is in the richest, most open and most extensible form it can be.

We use knowledge graphs for data storage, as a transfer format, when serialised, and as inputs to advanced reasoning and data processing tasks, such as for Machine Learning and Artificial Intelligence applications.""" ;
    schema:name
        "Kurrawong AI" ,
        "KurrawongAI" ;
.

<https://linked.data.gov.au/def/reg-statuses/stable>
